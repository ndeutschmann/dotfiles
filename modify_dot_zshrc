#!/bin/sh
tempfile="$(mktemp)"
trap 'rm -rf "${tempfile}"' EXIT
cat > "${tempfile}"

cat "${tempfile}"

set -- 'source ~/.local/share/yazi/y' 'export EDITOR="hx"' 'eval "$(zoxide init zsh)"' 'alias hux="uv run hx"'
for S in "$@" ; do
  if ! grep -Fxq "$S" "${tempfile}"; then
    echo "$S"
  fi
done
